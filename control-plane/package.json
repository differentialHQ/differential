    "@ts-rest/core": "^3.27.0",
    "@ts-rest/fastify": "^3.27.0",
    "@types/jest": "^29.5.4",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.12",
    "drizzle-orm": "^0.27.2",
